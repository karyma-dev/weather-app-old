   const { weather, main } = data\n\n            const newObj = {\n              weather: _.toLower(weather[0].main),\n              description: _.startCase(weather[0].description),\n              iconId: weather[0].icon,\n              temperature: Math.round(Number(main.temp) - 273.15) + '\\xB0 C',\n              humidity: main.humidity + '%'\n            }\n            state = newObj\n            console.log(state)\n            resolve(state)\n          })\n          .catch((error) => {\n            throw error\n          })\n      })\n    default:\n      return state\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"10662506fc81ca75fed4ccebdfaf85a8","cacheData":{"env":{}}}