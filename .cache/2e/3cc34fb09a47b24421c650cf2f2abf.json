            .get(\n                        `http://api.openweathermap.org/data/2.5/forecast?q=${action.payload}&APPID=29d3d4fa5f00dc1400ca4008a58633d4`\n                    )\n                    .then(({ data }) => {\n                        const arr = data.list.map((item) => {\n                            const dateObj = new Date(item.dt * 1000)\n                            const time = dateObj.toLocaleTimeString()\n                            item.day = dateObj.toDateString().slice(0, 3)\n                            item.date = dateObj.toDateString().slice(4, 10)\n                            item.time = time.slice(0, time.search(/\\D/)) + time.slice(time.search(/\\s/), 11)\n\n                            return item\n                        })\n\n                        state = arr\n\n                        resolve(state)\n                    })\n                    .catch((error) => {\n                        throw error\n                    })\n            })\n        default:\n            return state\n    }\n}\n"},"lineCount":null}},"error":null,"hash":"0e13dc9c4e089b46f3794a688f7deee5","cacheData":{"env":{}}}