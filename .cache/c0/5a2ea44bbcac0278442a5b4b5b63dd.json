ource":"components/Form.js","original":{"line":28,"column":2}},{"generated":{"line":76,"column":0},"source":"components/Form.js","original":{"line":45,"column":1}},{"generated":{"line":76,"column":1},"source":"components/Form.js","original":{"line":8,"column":0}},{"name":"Form","generated":{"line":78,"column":15},"source":"components/Form.js","original":{"line":47,"column":15}}],"sources":{"components/Form.js":"import React, { useState, useContext } from 'react'\nimport _ from 'lodash'\n\nimport { WeatherContext } from '../contexts/WeatherContext/index'\nimport { HourlyWeatherContext } from '../contexts/HourlyWeatherContext/index'\nimport searchIcon from '../img/search.svg'\n\nconst Form = () => {\n  const [city, setCity] = useState('')\n  const { weatherDispatch } = useContext(WeatherContext)\n  const { hourlyWeatherDispatch } = useContext(HourlyWeatherContext)\n\n  const onSubmit = (e) => {\n    e.preventDefault()\n    if (city) {\n      weatherDispatch({\n        type: 'GET_WEATHER',\n        payload: _.startCase(_.toLower(city))\n      })\n\n      hourlyWeatherDispatch({\n        type: 'GET_WEATHER',\n        payload: _.startCase(_.toLower(city))\n      })\n    }\n  }\n\n  return (\n    <form className=\"form\" onSubmit={onSubmit}>\n      <input\n        className=\"form-input\"\n        type=\"text\"\n        value={city}\n        onChange={(e) => setCity(e.target.value)}\n        placeholder=\"City Name\"\n        onFocus={(e) => (e.target.placeholder = '')}\n        onBlur={(e) => (e.target.placeholder = 'City Name')}\n      />\n\n      <button className=\"form-submit\">\n        <img src={searchIcon} alt=\"search icon\" />\n      </button>\n    </form>\n  )\n}\n\nexport default Form\n"},"lineCount":null}},"error":null,"hash":"13b5fa902fa739260a1a01fd33a0e32b","cacheData":{"env":{}}}